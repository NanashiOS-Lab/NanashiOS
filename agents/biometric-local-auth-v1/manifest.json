{
  "id": "biometric-local-auth-v1",
  "name": "Biometric Local Auth Agent",
  "description": "Gère l’authentification biométrique locale (visage, voix, empreinte) sans envoyer aucune donnée à l’extérieur. Très haut niveau de souveraineté et privacy.",
  "author": "NanashiOS-Lab",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "inputs": [
    {
      "name": "biometric_data",
      "type": "image or audio or file",
      "formats": ["jpg", "png", "wav"],
      "description": "Donnée biométrique (photo visage, enregistrement voix, etc.)"
    }
  ],
  "outputs": [
    {
      "name": "authenticated",
      "type": "bool",
      "description": "True si authentification réussie"
    },
    {
      "name": "confidence",
      "type": "float",
      "description": "Score de confiance (0-1)"
    },
    {
      "name": "user_id",
      "type": "string",
      "description": "Identifiant utilisateur local (si reconnu)"
    }
  ],
  "dependencies": ["face_recognition>=1.3.0", "librosa>=0.10.0", "opencv-python-headless>=4.5.0"],
  "price": "free",
  "tags": ["biometric", "authentication", "privacy", "local-auth", "security"]
}
